@model IEnumerable<flights.Entity.ticket>
@{
    Layout = "~/Areas/Admin/Views/shared/_LayoutAdmin.cshtml";

}

    <div class="midde_cont">
        <div class="container-fluid">
            <div class="row column_title">
                <div class="col-md-12">
                    <div class="page_title">
                        <h2> Tables</h2>
                    </div>
                </div>
            </div>
            <!-- row -->
                 <div class="row column1">
                <div class="col-md-12">
                    <div class="white_shd full margin_bottom_30">
                        <div class="full graph_head">
                            <div class="heading1 margin_0">
                                <h2> Tickets</h2>
                            </div>
                           
                           
                        </div>
                         <div class="full graph_head">
                                <a href="Create"><i class="fa-solid fa-circle-plus fa-3x green_color"></i> </a>
                            </div>
        <div class="container-fluid">

      <table id="example" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>NoOfSeats</th>
                <th>Status</th>
                <th>user Id</th>
                <th>Flight Number</th>        
               
                <th>operation</th>

            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model){
            <tr>
                <td>@item.NoOfSeats</td>
                <td>@item.CheckStatus</td>
             
                <td>@item.UserID</td>
                <td>@item.FlightNumber</td>
               
               
                
                <td>
                    <a href="Details/?id=@item.Id"><i class="fa-solid fa-3x fa-display mr-3  yellow_color"></i></a>

                    <a href="Update/?id=@item.Id"><i class="fa-solid fa-3x fa-pen-to-square mr-3"></i></a>

                    <a href="Delete/?id=@item.Id"><i class="fa-solid fa-3x fa-trash-can mr-3  red_color"></i></a>


                </td>


            </tr>
         
         
         }
           
          
        </tbody>
    
    </table>
    <a href="~/Admin/Home/tables" >Back to List</a>

    </div>
    </div>

                    </div>
                </div>
            </div>
            <!-- end row -->
        </div>
        <!-- footer -->
        <div class="container-fluid">
            <div class="footer">
                <p>Copyright © 2018 Designed by html.design. All rights reserved.</p>
            </div>
        </div>
    </div>
    <!-- end dashboard inner -->
 

